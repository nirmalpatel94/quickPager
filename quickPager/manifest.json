{
  "name": "Quick Pager",
  "version": "4.2.1",
  "manifest_version": 2,
  "description": "Quickly change proxy settings for pager",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Change Proxy Settings",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "https://localhost/*",
    "http://localhost/*",
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://localhost/; object-src 'self'"
}